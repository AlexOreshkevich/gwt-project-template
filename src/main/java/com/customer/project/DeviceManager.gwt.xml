<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.8.0//EN" "http://gwtproject.org/doctype/2.8.0/gwt-module.dtd">
<module rename-to='DeviceManager'>

	<!-- Inherit the core Web Toolkit stuff. -->
	<inherits name='com.google.gwt.user.User' />

	<inherits name='com.google.gwt.user.theme.standard.Standard' />

	<inherits name="com.google.gwt.logging.Logging" />
	<set-property name="gwt.logging.logLevel" value="INFO" />

	<!-- Specify the app entry point class. -->
	<entry-point class='com.customer.project.client.DeviceManager' />

	<set-property name="user.agent" value="safari" />

	<set-property name="compiler.emulatedStack" value="false" />

  <inherits name="com.google.gwt.i18n.I18N" />
	<extend-property name="locale" values="en" />

	<!-- Specify the paths for translatable code -->
	<source path='client' />
	<source path='shared' />

</module>